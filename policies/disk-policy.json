{
    "if": {
        "allOf": [
            {
                "field": "type",
                "equals": "Microsoft.Compute/virtualMachines/osDisk"
            },
            {
                "not": {
                    "field": "name",
                    "match": "[a-z0-9]{1,5}-[a-z0-9]{1,3}-vm[0-9]{1,3}-osdisk"
                }
            }
        ]
    },
    "then": {
        "effect": "audit"
    }
}